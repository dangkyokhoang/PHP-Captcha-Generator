{
  "name": "dkh/captcha-generator",
  "type": "library",
  "keywords": [
    "dkh",
    "php",
    "captcha",
    "generator"
  ],
  "description": "A PHP library for generating Captcha challenges using libgd.",
  "license": "MIT",
  "homepage": "https://github.com/dangkyokhoang/PHP-Captcha-Generator",
  "authors": [
    {
      "name": "Hoang",
      "homepage": "https://github.com/dangkyokhoang",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-gd": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*"
  },
  "autoload": {
    "psr-4": {
      "Dkh\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dkh\\": "tests"
    }
  }
}